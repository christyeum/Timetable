<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a4f.myapplication.Register"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">



    <Button
        android:id="@+id/btnEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edit"
        android:onClick="onEdit"
        android:layout_marginStart="82dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/btnEditCancel"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="82dp" />

    <Button
        android:id="@+id/btnEditCancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel"

        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="44dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="101dp"
        app:layout_constraintLeft_toRightOf="@+id/btnEdit"
        android:layout_marginLeft="44dp" />



    <EditText
        android:id="@+id/etName"
        android:layout_width="207dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint = "Name"
        android:inputType="textPersonName"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="145dp"
        android:layout_marginTop="82dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="145dp" />



    <EditText
        android:id="@+id/etId"
        android:layout_width="207dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint = "Username"
        android:inputType="textPersonName"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/etName"
        app:layout_constraintRight_toRightOf="@+id/etName"
        app:layout_constraintLeft_toLeftOf="@+id/etName"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp" />



    <EditText
        android:id="@+id/etPW"
        android:layout_width="207dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint = "Password"
        android:inputType="textPassword"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/etId"
        app:layout_constraintRight_toRightOf="@+id/etId"
        app:layout_constraintLeft_toLeftOf="@+id/etId"/>



    <EditText
        android:id="@+id/etPW2"
        android:layout_width="208dp"
        android:layout_height="46dp"
        android:ems="10"
        android:hint = "Password confirm"
        android:inputType="textPassword"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/etPW"
        app:layout_constraintRight_toRightOf="@+id/etPW"
        app:layout_constraintLeft_toLeftOf="@+id/etPW"/>



    <EditText
        android:id="@+id/etDept"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="Department"
        android:inputType="textPersonName"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/etPW2"
        app:layout_constraintRight_toRightOf="@+id/etPW2"
        app:layout_constraintLeft_toLeftOf="@+id/etPW2" />




    <EditText
        android:id="@+id/etGrade"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Current Grade"
        android:inputType="textPersonName|numberDecimal"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/etDept"
        app:layout_constraintRight_toRightOf="@+id/etDept"
        app:layout_constraintLeft_toLeftOf="@+id/etDept"
        android:layout_marginTop="1dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:text="confirm"
        android:textSize="20sp"
        app:layout_constraintRight_toLeftOf="@+id/etPW2"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="3dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView7"
        android:layout_marginRight="3dp"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/btnEdit"
        android:layout_marginTop="217dp"
        android:layout_marginBottom="218dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:text="Grade"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView9"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:text="Dept"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView7"
        android:layout_marginTop="57dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:text="PW"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView4"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:text="ID"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintRight_toRightOf="@+id/textView3"
        android:layout_marginTop="50dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="57dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="@+id/etName"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="71dp"
        android:layout_height="35dp"
        android:text="Name"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="97dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="50dp"
        android:layout_marginRight="28dp" />

</android.support.constraint.ConstraintLayout>
